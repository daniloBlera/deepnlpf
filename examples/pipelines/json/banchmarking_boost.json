{
    "lang": "en",
    "tools": {
        "stanza": {
            "processors": [
                "tokenize",
                "mwt",
                "pos",
                "lemma",
                "ner",
                "depparse"
            ]
        },
        "semafor": {
            "processors": [
                "frame_semantic_parsing"
            ]
        },
        "spacy": {
            "processors": [
                "pos",
                "tag",
                "shape",
                "is_alpha",
                "is_title",
                "like_num",
                "label"
            ]
        },
        "stanfordcorenlp": {
            "processors": [
                "tokenize",
                "ssplit",
                "pos",
                "lemma",
                "ner",
                "truecase",
                "parse",
                "depparse"
            ]
        }
    }
}